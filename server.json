{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "io.github.techops-services/keeperhub-mcp",
  "description": "MCP server for KeeperHub blockchain workflow automation",
  "repository": {
    "url": "https://github.com/techops-services/keeperhub-mcp",
    "source": "github"
  },
  "version": "1.0.0",
  "remotes": [
    {
      "type": "sse",
      "url": "https://mcp.keeperhub.com/sse"
    }
  ],
  "packages": [
    {
      "registryType": "npm",
      "identifier": "keeperhub-mcp",
      "version": "1.0.0",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [
        {
          "name": "KEEPERHUB_API_KEY",
          "description": "Your KeeperHub API key for authentication",
          "isRequired": true,
          "isSecret": true
        }
      ]
    }
  ]
}
